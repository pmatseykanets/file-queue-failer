{
    "name": "pmatseykanets/file-queue-failer",
    "description": "File based implementation of Laravel Queue Failer",
    "keywords": ["laravel", "lumen", "queue", "failer", "failed", "job"],
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Matseykanets",
            "email": "pmatseykanets@gmail.com",
            "homepage": "https://github.com/pmatseykanets",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/contracts": "5.3.*|5.4.*|5.5.*",
        "illuminate/queue": "5.3.*|5.4.*|5.5.*",
        "illuminate/support": "5.3.*|5.4.*|5.5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Pvm\\FileQueueFailer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pvm\\FileQueueFailer\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pvm\\FileQueueFailer\\Queue\\QueueServiceProvider"
            ]
        }
    }
}
